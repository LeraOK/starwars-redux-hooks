SWContext)\n\n    useEffect(() =>{\n        let key = props.match.params.hero;\n        if(!keys.includes(key)){\n            key = defaultHero;\n        }\n        let hero = JSON.parse(localStorage.getItem(key));\n        if (!hero || (Date.now() - hero.time) > periodWeek) {\n            fetch(characters[key].url)\n                .then(response => response.json())\n                .then(data => {\n                    let info = {\n                        \"name\": data.name,\n                        \"height\": data.height,\n                        \"mass\": data.mass,\n                        \"hair_color\": data.hair_color,\n                        \"skin_color\": data.skin_color,\n                        \"eye_color\": data.eye_color,\n                        \"birth_year\": data.birth_year,\n                        \"gender\": data.gender\n                    };\n                    setHero(info);\n                    hero = {\n                        info,\n                        time: Date.now()\n                    };\n                    localStorage.setItem(key, JSON.stringify(hero));\n                });\n        } else {\n            setHero(hero.info);\n        }\n        changeHero(key);\n    })\n        return (\n            <div>\n                {(hero) &&\n                <div className={`farGalaxy ${styles.hero_box}`}>\n                    <p><span className={styles.hero_titles}>name:</span> {hero.name}</p>\n                    <p><span className={styles.hero_titles}>height:</span> {hero.height}</p>\n                    <p><span className={styles.hero_titles}>birth year:</span> {hero.birth_year}</p>\n                    <p><span className={styles.hero_titles}>gender:</span> {hero.gender}</p>\n                    <p><span className={styles.hero_titles}>mass:</span> {hero.mass}</p>\n                    <p><span className={styles.hero_titles}>hair color:</span> {hero.hair_color}</p>\n                    <p><span className={styles.hero_titles}>skin color:</span> {hero.skin_color}</p>\n                    <p><span className={styles.hero_titles}>eye color:</span> {hero.eye_color}</p>\n                </div>\n                }\n            </div>\n        )\n}\n\nexport default AboutMe;","/Users/macbook/Documents/TelRan/Part_2/starwars-redux-hooks/src/components/Navigation.js",[],"/Users/macbook/Documents/TelRan/Part_2/starwars-redux-hooks/src/components/StarWars.js",[],"/Users/macbook/Documents/TelRan/Part_2/starwars-redux-hooks/src/components/Contact.js",[],"/Users/macbook/Documents/TelRan/Part_2/starwars-redux-hooks/src/components/ErrorPage.js",[],"/Users/macbook/Documents/TelRan/Part_2/starwars-redux-hooks/src/components/Hero.js",[],"/Users/macbook/Documents/TelRan/Part_2/starwars-redux-hooks/src/components/DreamTeam.js",[],"/Users/macbook/Documents/TelRan/Part_2/starwars-redux-hooks/src/components/FarGalaxy.js",["83"],"import React, {Component, useEffect, useState} from 'react';\nimport style from \"../css_modules/fargalaxy.module.css\";\nimport {base_url} from \"../utils/constants\";\n\nconst FarGalaxy = () => {\n    const [isLoading, setIsLoading] = useState(true);\n    const [opening_crawl, setOpening_crawl] = useState();\n    //первый аргумент коммонет дид маунт\n    //второй аргумент компонент дид апдейт\n    //ретерн за компонент вилл анмаунт\n    useEffect(() => {\n        const opening_crawl = sessionStorage.getItem('opening_crawl');\n        if (opening_crawl) {\n            setIsLoading(false);\n            setOpening_crawl(opening_crawl)\n        } else {\n            const episode = Math.floor(Math.random() * 6) + 1;\n            fetch(`${base_url}/v1/films/${episode}`)\n                .then(response => response.json())\n                .then(data => {\n                    setIsLoading(false);\n                    setOpening_crawl(data.opening_crawl);\n                    sessionStorage.setItem('opening_crawl', data.opening_crawl);\n                });\n        }\n    },[]);\n    const text = isLoading ? 'Loading...' : opening_crawl;\n    return (\n        <p className={`${style.farGalaxy} text-justify`}>{text}</p>\n    );\n}\n\nexport default FarGalaxy;","/Users/macbook/Documents/TelRan/Part_2/starwars-redux-hooks/src/components/Friend.js",[],["84","85"],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","severity":1,"message":"91","line":17,"column":7,"nodeType":"92","endLine":17,"endColumn":32,"suggestions":"93"},{"ruleId":"90","severity":1,"message":"94","line":10,"column":5,"nodeType":"95","endLine":10,"endColumn":14,"suggestions":"96"},{"ruleId":"97","severity":1,"message":"98","line":1,"column":16,"nodeType":"95","messageId":"99","endLine":1,"endColumn":25},{"ruleId":"86","replacedBy":"100"},{"ruleId":"88","replacedBy":"101"},"no-native-reassign",["102"],"no-negated-in-lhs",["103"],"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'changeHero' and 'key'. Either include them or remove the dependency array.","ArrayExpression",["104"],"React Hook useEffect contains a call to 'setHero'. Without a list of dependencies, this can lead to an infinite chain of updates. To fix this, pass [props.match.params.hero, changeHero] as a second argument to the useEffect Hook.","Identifier",["105"],"no-unused-vars","'Component' is defined but never used.","unusedVar",["102"],["103"],"no-global-assign","no-unsafe-negation",{"desc":"106","fix":"107"},{"desc":"108","fix":"109"},"Update the dependencies array to be: [changeHero, key, props.match.params.hero]",{"range":"110","text":"111"},"Add dependencies array: [props.match.params.hero, changeHero]",{"range":"112","text":"113"},[568,593],"[changeHero, key, props.match.params.hero]",[1524,1524],", [props.match.params.hero, changeHero]"]